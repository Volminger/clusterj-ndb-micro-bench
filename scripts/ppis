#!/bin/bash
set -e
DIR=$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )
source $DIR/setup
java -Xmx20G  -XX:MaxDirectMemorySize=1000m -jar $DIR/../target/clusterj-ndb-micro-benchmark-1.0-jar-with-dependencies.jar -schema $db -dbHost $dbHost -clientId $clientId -rowsPerTx $rowsPerTx -numThreads $numThreads -microBenchType PPIS  -maxOperationsToPerform $totalOps $skipPrompt
